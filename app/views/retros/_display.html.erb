<% @quad.each do |n| %> 
	<%= link_to team_retro_note_path(@team, @retro, @notes.find(params[:id])) do %> 
			<%= if !n.conclussion.nil? && !n.conclussion.empty? then "ðŸ””" end %> 
			<% if n.complete_status == "1" %>

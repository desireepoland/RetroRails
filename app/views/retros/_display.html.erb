			<% end %> 
			<% if !n.upvote.nil? %> 
				+<div class="upvote"><%= n.upvote %></div>
			<% end %> 
			<div class="update-2"><%= link_to "Edit", edit_team_retro_note_path(current_team, @retro, Note.find(params[:id])) %>
